{
  "name": "squaredcoffee-server",
  "version": "1.0.0",
  "description": "clovered server",
  "main": "none",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev_migrate": "RAILS_ENV=development rake db:create_migrate",
    "dev": "npm run dev_migrate && RAILS_ENV=development bundle exec rails s -b 0.0.0.0",
    "start": "bundle exec rails s -b 0.0.0.0",
    "dump": "rake db:dump format=tar",
    "migrate": "RAILS_ENV=production rake db:create_migrate",
    "start_prod": "RAILS_ENV=production npm run dump && npm run migrate && bundle exec foreman start",
    "drop" : "rake db:drop db:reset db:create db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronanamsterdam/squaredcoffee-server.git"
  },
  "author": "Roman Zhyliov",
  "license": "BSD 3-Clause",
  "bugs": {
    "url": "https://github.com/ronanamsterdam/squaredcoffee-server/issues"
  },
  "homepage": "https://github.com/ronanamsterdam/squaredcoffee-server#readme"
}
